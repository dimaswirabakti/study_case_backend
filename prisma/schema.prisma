generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Menu {
  id          Int      @id @default(autoincrement())
  name        String
  category    String
  calories    Int
  price       Float
  ingredients Json
  description String?
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt

  @@map("menus")
}